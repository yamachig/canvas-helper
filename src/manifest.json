{
    "name": "canvas-transcript-detacher",
    "version": "0.1.0",
    "description": "Detach transcript on Canvas Media Gallery",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["main.js"],
            "all_frames": true
        }
    ]
}